<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.pc.rutacbc.VistaGaleria"
    tools:showIn="@layout/app_bar_vista_galeria">
<TextView
    android:id="@+id/txv_agregar_titulacion"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="@string/txv_agregar_titulacion"
    android:layout_marginTop="30dp"
    android:layout_centerHorizontal="true"
    android:textSize="@dimen/txv_login"
    android:textColor="@color/colorarriba"/>

    <ImageButton
        android:id="@+id/btn_galeria"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_marginLeft="290dp"
        android:layout_marginTop="20dp"
        app:srcCompat="@drawable/ic_menu_camera"
        android:background="@color/txv_agregar_titulacion"/>
</RelativeLayout>
